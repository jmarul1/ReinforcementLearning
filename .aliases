# repo
repo="/c/Code_Repository/github/market"

# proxy
pproxy='http://proxy-chain.intel.com:911'
http_proxy=$pproxy
https_proxy=$pproxy
export http_proxy
export https_proxy

# commands
alias tree='find . -print | sed -e "s;[^/]*/;|____;g;s;____|; |;g"'
